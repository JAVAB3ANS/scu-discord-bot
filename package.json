{
  "name": "scu-discord-bot",
  "version": "1.0.0",
  "description": "![SCU Discord Network Banner](https://github.com/JAVAB3ANS/scu-discord-bot/blob/master/assets/scu_banner.png?raw=true)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node .",
    "start": "pm2 start index.js && pm2 log",
    "restart": "pm2 restart index.js && pm2 log",
    "start-all": "pm2 start index.js && pm2 start main.ts --interpreter='deno' --interpreter-args='run -A --allow-net --allow-read' && pm2 log",
    "restart-all": "pm2 restart index.js && pm2 restart main.ts --interpreter='deno' --interpreter-args='run -A --allow-net --allow-read' && pm2 log"
  },
  "dependencies": {
    "ansi-regex": "^5.0.1",
    "degenerator": "^5.0.1",
    "discord.js": "^12.5.3",
    "discord.js-commando": "^0.12.3",
    "dot": "^1.1.3",
    "follow-redirects": "^1.15.6",
    "http-cache-semantics": "^4.1.1",
    "jsdom": "^16.7.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "node-gyp": "^10.2.0",
    "nodemailer": "^6.9.14",
    "npm": "^10.8.2",
    "nth-check": "^2.0.1",
    "nwsapi": "^2.2.12",
    "pac-resolver": "^7.0.1",
    "pm2": "^5.4.2",
    "quick.db": "^7.1.3",
    "simple-get": "^4.0.1",
    "tar": "^7.4.3",
    "vm2": "^3.9.19"
  },
  "author": "Jason Vu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JAVAB3ANS/scu-discord-bot.git"
  },
  "bugs": {
    "url": "https://github.com/JAVAB3ANS/scu-discord-bot/issues"
  },
  "homepage": "https://github.com/JAVAB3ANS/scu-discord-bot#readme"
}
